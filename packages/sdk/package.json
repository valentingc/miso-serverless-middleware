{
  "name": "@miso/sdk",
  "version": "0.10.0",
  "description": "",
  "main": "dist/index.js",
  "private": true,
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.7.3",
    "@grpc/proto-loader": "^0.7.10",
    "@kubernetes/client-node": "^0.18.1",
    "@miso/common": "^0.9.0",
    "@types/winston": "^2.4.4",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "copy-proto-files": "copyfiles -f ../common/src/grpc/*.proto dist/",
    "test": "jest",
    "build": "npm run clean && tsc --build && npm run copy-proto-files",
    "release": "tsc --build && npm version patch && npm publish"
  },
  "author": "MISO Authors",
  "license": "Apache-2.0",
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
